package classe;

public class FuncionarioTeste {
	

	public static void main(String[] args) {
		
		Funcionario ficha = new Funcionario("José", 1250.50, "Cobrador" );
	
		System.out.println(ficha.format());
		
		System.out.println("\nFicha");
		System.out.println("\nNome: " + ficha.name);
		System.out.println("\nSalário: " + ficha.sal);
		System.out.println("\nFunção: " + ficha.func);
	
	}
	

}
